{
  "$schema": "https://raw.githubusercontent.com/simeg/urlsup/main/schema.json",
  "description": "High-performance configuration for large repositories with many URLs",
  "timeout": 5,
  "retry": 1,
  "retry_delay": 500,
  "concurrency": 50,
  "rate_limit": 50,
  "allow_status": [200, 202, 204, 301, 302, 429],
  "allowlist": [
    "github.com",
    "*.github.com",
    "docs.github.com",
    "api.github.com",
    "raw.githubusercontent.com",
    "gist.github.com",
    "stackoverflow.com",
    "developer.mozilla.org",
    "w3.org",
    "*.w3.org"
  ],
  "exclude_patterns": [
    "localhost",
    "127\\.0\\.0\\.1",
    "\\.local",
    "staging\\.",
    "dev\\.",
    "test\\.",
    "mock\\.",
    "example\\.com",
    "placeholder\\.",
    "slow-?endpoint",
    "timeout-?test",
    "delay\\.",
    "sleep\\."
  ],
  "user_agent": "fast-url-checker/1.0 (+https://github.com/your-org/your-repo)",
  "include": ["md", "rst", "txt"],
  "exclude": [
    "node_modules/",
    ".git/",
    "build/",
    "dist/",
    "coverage/",
    "logs/",
    "tmp/",
    "temp/",
    ".cache/",
    ".vscode/",
    ".idea/",
    "__pycache__/",
    ".pytest_cache/",
    "vendor/",
    "target/",
    "bin/",
    "obj/",
    ".nuget/",
    "packages/"
  ],
  "failure_threshold": 5
}